    <form name="myform">
        <div id="ProjectSelectionSheetVieweditView" style="display:none;">
            <div style="font-weight:bold;font-size:16px;padding:0.5%;float:right;cursor:pointer;" ng-click="Close();" id="close">X</div>
            <div style="clear:both"></div>
            <div class="col-sm-12 col-md-6 col-lg-6" style="margin:1% 25%;">
                <div class="form-group m-2">
                    <span class="w-50 float-left text-left pr-1" style="font-size:14px;line-height:35px;">
                        TITLE OF THE QCC  CASE STUDY
                    </span><span style="float:left;width:4%;line-height:35px;">:-</span><span style="width:46%;float:left;">
                        <input class="form-control float-left inputaddvalues" placeholder="Enter Case Study Title" type="text" name="" ng-model="Title" ng-required="true" style="font-size:1.5rem;" />
                    </span>
                </div>

                <div class="form-group m-2">
                    <span class="w-50 float-left text-left pr-1" style="font-size:14px;line-height:35px;">
                        OBJECTIVE
                    </span><span style="float:left;width:4%;line-height:35px;">:-</span><span style="width:46%;float:left;">
                        <input class="form-control float-left inputaddvalues" placeholder="Enter OBJECTIVE" type="text" name="" ng-model="Objective" ng-required="true" style="font-size:1.5rem;" />
                    </span>
                </div>
                <div ng-hide="ProjectInitiationSelection">
                    <div class="form-group m-2">
                        <span class="w-50 float-left text-left pr-1" style="font-size:14px;line-height:35px;">
                            GOAL FROM
                        </span><span style="float:left;width:4%;line-height:35px;">:-</span><span style="width:46%;float:left;">
                            <input class="form-control float-left inputaddvalues" placeholder="Enter Goal From" type="text" name="" ng-model="Goal" ng-required="true" style="font-size:1.5rem;" />
                            <input class="form-control float-left inputaddvalues" placeholder="UOM" type="text" name="" ng-model="GoalUOM" ng-required="true" ng-blur="GaolUOM()" style="text-transform: uppercase;font-size:1.5rem;" />

                        </span>
                    </div>
                    <div class="form-group m-2">
                        <span class="w-50 float-left text-left pr-1" style="font-size:14px;line-height:35px;">
                            TO
                        </span><span style="        float: left;
        width: 4%;
        line-height: 35px;
">:-</span><span style="width:46%;float:left;">
                            <input class="form-control float-left inputaddvalues" placeholder="Enter Goal To" type="text" name="" ng-model="GoalTo" ng-required="true" style="font-size:1.5rem;" />
                            <input class="form-control float-left inputaddvalues" type="text" placeholder="UOM" name="" ng-model="GoalToUOM" ng-disabled="true" style="text-transform:uppercase;font-size:1.5rem;" />

                        </span>
                    </div>
                    <div class="form-group m-2">
                        <span class="w-50 float-left text-left pr-1" style="font-size:14px;line-height:35px;">
                            PROJECT START DATE
                        </span><span style="float:left;width:4%;line-height:35px;">:-</span><span style="width:46%;float:left;">
                            <input id="datepicker-range-start" style="width:222px;font-size:1.5rem;" type="text" class="form-control float-left inputaddvalues" data-zdp_readonly_element="false" placeholder="Select Project Year-From" ng-required="true">
                        </span>
                    </div>

                    <div class="form-group m-2">
                        <span class="w-50 float-left text-left pr-1" style="font-size:14px;line-height:35px;" ng-required="true">
                            PROJECT END DATE
                        </span><span style="float:left;width:4%;line-height:35px;">:-</span><span style="width:46%;float:left;">
                            <input id="datepicker-range-end" type="text" style="width:222px;font-size:1.5rem;" class="form-control float-left inputaddvalues" data-zdp_readonly_element="false" placeholder="Select Project Year-To">
                        </span>
                    </div>

                    <div class="form-group m-2">
                        <span class="w-50 float-left text-left pr-1" style="font-size:14px;line-height:35px;">
                            ESTIMATED COST SAVING
                        </span><span style="float:left;width:4%;line-height:35px;">:-</span><span style="width:46%;float:left;">
                            <input class="form-control float-left inputaddvalues" placeholder="Enter Estimated Cost Saving" type="text" name="" ng-model="ExpectedCostSaving" style="font-size:1.5rem;" />
                        </span>
                    </div>
                </div>
                <div ng-hide="ProjectInitiation">
                    <div class="form-group m-2">
                        <span class="w-50 float-left text-left pr-1" style="font-size:14px;line-height:35px;">
                            ACTUAL GOAL
                        </span><span style="float:left;width:4%;line-height:35px;">:-</span><span style="width:46%;float:left;">
                            <input class="form-control float-left inputaddvalues" type="text" name="" ng-model="ActualGoal" style="font-size:1.5rem;" />
                        </span>
                    </div>

                    <div class="form-group m-2">
                        <span class="w-50 float-left text-left pr-1" style="font-size:14px;line-height:35px;">
                            ACTUAL PROJECT START DATE
                        </span><span style="float:left;width:4%;line-height:35px;">:-</span><span style="width:46%;float:left;">
                            <input id="datepicker-range-APstart" style="width:222px;font-size:1.5rem;" type="text" class="form-control float-left inputaddvalues" data-zdp_readonly_element="false" placeholder="Select Project Year-From">
                        </span>
                    </div>

                    <div class="form-group m-2">
                        <span class="w-50 float-left text-left pr-1" style="font-size:14px;line-height:35px;">
                            ACTUAL PROJECT END DATE
                        </span><span style="float:left;width:4%;line-height:35px;">:-</span><span style="width:46%;float:left;">
                            <input id="datepicker-range-APend" style="width:222px;font-size:1.5rem;" type="text" class="form-control float-left inputaddvalues" data-zdp_readonly_element="false" placeholder="Select Project Year-To">
                        </span>
                    </div>

                    <div class="form-group m-2">
                        <span class="w-50 float-left text-left pr-1" style="font-size:14px;line-height:35px;">
                            ACTUAL COST SAVING
                        </span><span style="float:left;width:4%;line-height:35px;">:-</span><span style="width:46%;float:left;">
                            <input class="form-control float-left inputaddvalues" type="text" name="" ng-model="ActualExpectedCostSaving" style="font-size:1.5rem;" />
                        </span>
                    </div>
                </div>
            </div>
            <div class="col-sm-12 col-md-12 col-lg-12 text-center">
                <button class="btn btn-success" style="font-size:12px;font-weight:bold;" ng-click="Close();">Cancel</button>
                <button class="btn btn-success" style="font-size:12px;font-weight:bold;" ng-click="myform.$valid &&Submit()">Submit</button>
            </div>

