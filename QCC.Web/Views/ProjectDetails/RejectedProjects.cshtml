    <div id="ProjectSelectionSheetView" style="display:block; background:none;">
        @*<h3 style="width:96%;text-align:center;float:left;">Team Details</h3>*@
        @*<div style="font-weight: bold; font-size: 16px; padding: 0.5%; float: right; cursor: pointer;" ng-click="Close();" id="close">X</div>*@

        <div style="clear:both"></div>
        <br />
        <div class="form-group w-100">
            <input type="text" ng-model="search" class="form-control" placeholder="Search" style="width:20%;padding: 0.375rem 1.75rem;font-size: 1.4rem;float:left" />
            @*<input type="text" class="form-control" placeholder="Search for..." name="mySearch" ng-model="search">*@
            <div style="float:right">
                View <select ng-model="viewby" ng-change="setItemsPerPage(viewby)" id="viewby"><option>3</option><option>5</option><option>10</option><option>20</option><option>30</option><option>40</option><option>50</option><option>75</option><option>100</option></select> records at a time.
            </div>
        </div>
        <table class="table table-bordered">
            <thead class="bg-info" style="color:#fff;font-weight:bold;text-align:center;font-size:14px;">
                <tr>
                    <th>SL.NO</th>
                    <th>Title</th>
                    <th>Objective</th>
                    <th>Goal From</th>
                    <th>Goal To</th>
                    <th>Project Start Date</th>
                    <th>Project End Date</th>
                    <th>Expected Cost Saving</th>
                    <th>Actual Goal</th>
                    <th>Actual Project Start Date</th>
                    <th>Actual Project End Date</th>
                    <th>Actual Cost Saving</th>
                    <th>Remarks</th>
                    <th>Status</th>
                    <th>View</th>
                </tr>
            </thead>
            <tbody>
                <tr ng-repeat="ProjectDetails in RejectedResponse" class="project_{{ProjectDetails.ProjectId}}">
                    <th scope="row">
                        {{$index + 1}}
                    </th>
                    <td ng-hide="true">
                        {{ProjectDetails.ProjectId}}
                    </td>
                    <td ng-hide="true">
                        {{ProjectDetails.TeamId}}
                    </td>
                    <td>
                        {{ProjectDetails.Title}}
                    </td>
                    <td>
                        {{ProjectDetails.Objective}}
                    </td>
                    <td>
                        {{ProjectDetails.Goal}}
                    </td>
                    <td>
                        {{ProjectDetails.GoalTo}}
                    </td>
                    <td>
                        {{ProjectDetails.ProjectStartDate}}
                    </td>
                    <td>
                        {{ProjectDetails.ProjectEndDate}}
                    </td>
                    <td>
                        {{ProjectDetails.ExpectedCostSaving}}
                    </td>
                    <td>
                        {{ProjectDetails.ActualGoal}}
                    </td>
                    <td>
                        {{ProjectDetails.ActualProjectStartDate}}
                    </td>
                    <td>
                        {{ProjectDetails.ActualProjectEndDate}}
                    </td>
                    <td>
                        {{ProjectDetails.ActualExpectedCostSaving}}
                    </td>
                    <td>
                        {{ProjectDetails.Remarks}}
                    </td>
                    <td ng-hide="true">
                        {{ProjectDetails.Status}}
                    </td>
                    <td>
                        {{ProjectDetails.StatusName}}
                    </td>
                    <td style="text-align:center">
                        <span ng-click="toggleEdit1(ProjectDetails)" class="glyphicon glyphicon-pencil aprovedviewBtn" style="cursor:pointer"></span>
                        <span ng-click="delete(ProjectDetails.ProjectId)" class="glyphicon glyphicon-trash aprovedviewBtn" style="cursor:pointer"></span>
                    </td>
                </tr>
            </tbody>
