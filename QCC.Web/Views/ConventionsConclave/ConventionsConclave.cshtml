            //$(".internalbtn").css({ "width": "16rem" });
            $(".Overal_conventions").css({ "font-weight": "none" })
            $(this).css({ "box-shadow": "rgb(0, 0, 0) 0px 5px 10px", "font-weight": "bold" });
            $(".Overal_conventions").css({ "box-shadow": "" });
        })
        $(".ICQCC_conventions").on("click", function () {
            $(".Internal_conventions,.Chapter_conventions,.ICQCC_conventions,.National_conventions,.Inter_National_conventions").css({ "box-shadow": "none", "font-weight": "400" });
            $(".conventionsPage").hide();
            $("#ICQCC_conventions").show();
            //$(".overalbtn").css({ "width": "20rem" });
            $(".Overal_conventions").css({ "font-weight": "none" })
            //$(".internalbtn").css({ "width": "16rem" });
            //$(".icqccbtn").css({ "width": "21rem" });
            //$(".internationalbtn").css({"width":"19rem"})
            $(this).css({ "box-shadow": "rgb(0, 0, 0) 0px 5px 10px", "font-weight": "bold" });
            $(".Overal_conventions").css({ "box-shadow": "" });
        })
        $(".Inter_National_conventions").on("click", function () {
            $(".Internal_conventions,.Chapter_conventions,.ICQCC_conventions,.National_conventions,.Inter_National_conventions").css({ "box-shadow": "none", "font-weight": "400" });
            $(".conventionsPage").hide();
            $("#Inter_National_conventions").show();
            //   $(".overalbtn").css({ "width": "20rem" });

            // $(".internalbtn").css({ "width": "16rem" });
            //$(".icqccbtn").css({ "width": "19rem" });
            //$(".internationalbtn").css({ "width": "21rem" });

            $(this).css({ "box-shadow": "rgb(0, 0, 0) 0px 5px 10px", "font-weight": "bold" });
            $(".Overal_conventions").css({ "box-shadow": "", "font-weight": "normal" });

