            $(".Overal_conventions").css({ "box-shadow": "" });
            $(".Overal_conventions").css({ "font-weight": "none" })
        })
        $(".National_conventions").on("click", function () {
            $(".Internal_conventions,.Chapter_conventions,.ICQCC_conventions,.National_conventions,.Inter_National_conventions").css({ "box-shadow": "none", "font-weight": "400" });
            $(".conventionsPage").hide();
            $("#National_conventions").show();
