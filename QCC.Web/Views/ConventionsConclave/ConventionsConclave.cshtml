            //$(".overalbtn").css({ "width": "22rem" });
            //$(".internalbtn").css({ "width": "16rem" });
            //$(".internationalbtn").css({ "width": "19rem" });
            $(this).css({ "box-shadow": "rgb(0, 0, 0) 0px 5px 10px", "font-weight": "bold" });

        });
        $(".Home > a").mouseenter(function () {
            // alert("hello");
            $(this).css({ "background": "rgba(0, 123, 194,1)" });
        });
        $(".Home > a").mouseleave(function () {
